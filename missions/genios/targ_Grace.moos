{
  AppTick   = 1
  CommsTick = 1
  swath_width = 20
  swath_length = 20
  show_swath = true
  sensor_pd   = 1
  report_name = Grace
}


//--------------------------------------------------
// uFldNodeBroker configuration block from plugin

