  SPEED_PID_KP           = 1.0
  SPEED_PID_KD           = 0.0
  SPEED_PID_KI           = 0.0
  SPEED_PID_INTEGRAL_LIMIT = 0.07
  

  // Maximums
  MAXRUDDER  = 100
  MAXTHRUST  = 100
  
